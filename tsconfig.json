{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@cloud/*": [
        "apps/cloud/src/*"
      ],
      "@web/*": [
        "apps/web/src/*"
      ]
    }
  }
}